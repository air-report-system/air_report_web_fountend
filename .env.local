# 环境变量配置示例
# 复制此文件为 .env.local 并填入实际值

# 应用基础配置
NODE_ENV=production
PORT=3000

# 后端API配置
# 替换为实际的后端Replit URL
# BACKEND_URL=https://airreportwebbackend-aliceaierlanta.replit.app
# NEXT_PUBLIC_API_URL=https://airreportwebbackend-aliceaierlanta.replit.app/api/v1
BACKEND_URL=http://192.168.176.1:8000
NEXT_PUBLIC_API_URL=http://192.168.176.1:8000/api/v1


# Next.js配置
NEXT_PUBLIC_APP_NAME=室内空气检测数据处理系统
NEXT_PUBLIC_APP_VERSION=1.0.0

# 可选：自定义域名配置
# NEXT_PUBLIC_DOMAIN=your-custom-domain.com

# 调试配置 (开发环境)
DEBUG=true
NEXT_PUBLIC_DEBUG=true

# 安全配置
# NEXTAUTH_SECRET=your-secret-key-here
# NEXTAUTH_URL=https://your-frontend-url.replit.app

# 文件上传配置
# NEXT_PUBLIC_MAX_FILE_SIZE=10485760  # 10MB
# NEXT_PUBLIC_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif

# 性能配置
# NEXT_PUBLIC_API_TIMEOUT=30000  # 30秒
