{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p ${PORT:-3000} -H ${HOSTNAME:-0.0.0.0}",
    "lint": "next lint",
    "replit:start": "chmod +x start.sh && ./start.sh",
    "health-check": "node health-check.js",
    "cors-test": "node cors-test.js",
    "deploy": "npm install --legacy-peer-deps && npm run build && npm run start",
    "quick-start": "chmod +x quick-start.sh && ./quick-start.sh",
    "test:connection": "npm run health-check && npm run cors-test",
    "test:nix": "chmod +x test-nix-packages.sh && ./test-nix-packages.sh",
    "install:replit": "npm install --legacy-peer-deps",
    "clean:install": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "fix:deps": "chmod +x fix-dependencies.sh && ./fix-dependencies.sh"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tanstack/react-query": "^5.81.2",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.523.0",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "typescript": "^5"
  }
}